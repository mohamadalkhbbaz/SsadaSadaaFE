
        public string GetByValAndTypeTest(string type, string val)
        {
            var model = DefaultTtglistSeedData.ttgList.FirstOrDefault(x => x.LIST_ITEM.ToLower() == type.ToLower() && x.LIST_VAL == val);
            return L(model);
        }



